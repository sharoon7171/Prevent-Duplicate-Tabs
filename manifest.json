{
  "manifest_version": 3,
  "name": "Prevent Duplicate Tabs",
  "version": "1.0.0",
  "description": "Prevent duplicate tabs from being opened",
  "author": "SQ Tech",
  "homepage_url": "https://sqtech.dev",
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "host_permissions": [],
  "background": {
    "service_worker": "background/background.mjs",
    "type": "module"
  },
  "content_scripts": [],
  "icons": {
    "16": "assets/icons/icon16.png",
    "32": "assets/icons/icon32.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "action": {
    "default_title": "Prevent Duplicate Tabs",
    "default_icon": {
      "16": "assets/icons/icon16.png",
      "32": "assets/icons/icon32.png",
      "48": "assets/icons/icon48.png",
      "128": "assets/icons/icon128.png"
    }
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  }
}
